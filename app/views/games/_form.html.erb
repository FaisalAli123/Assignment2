<!-- Game Form -->
<%= simple_form_for @game, :html => { :multipart => true } do |f| %>
  <%= select_tag(:filter_id, options_for_select(@filters), :prompt => "Select A Genre") %>
  <%= f.file_field :game_img %>
  <%= f.input :name, label: "Game Name" %>
  <%= f.input :description %>
  <%= f.input :developer %>
  <%= f.button :submit %>
<% end %>
